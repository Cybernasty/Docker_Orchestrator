{
  "builder": {
    "gc": {
      "defaultKeepStorage": "20GB",
      "enabled": true
    }
  },
  "experimental": false,
  "hosts": [
    "tcp://0.0.0.0:2375",
    "npipe:////./pipe/docker_engine"
  ],
  "registry-mirrors": [],
  "insecure-registries": [
    "localhost:6500",
    "192.168.1.3:6500"
  ],
  "debug": false,
  "features": {
    "buildkit": true
  },
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "10m",
    "max-file": "3"
  },
  "storage-driver": "overlay2",
  "storage-opts": [
    "overlay2.override_kernel_check=true"
  ],
  "max-concurrent-downloads": 3,
  "max-concurrent-uploads": 5,
  "default-ulimits": {
    "nofile": {
      "Hard": 64000,
      "Name": "nofile",
      "Soft": 64000
    }
  },
  "live-restore": true,
  "userland-proxy": false,
  "ip-forward": true,
  "iptables": true,
  "ip-masq": true,
  "selinux-enabled": false,
  "userns-remap": "",
  "group": "docker",
  "default-cgroupns-mode": "host",
  "default-address-pools": [
    {
      "base": "172.17.0.0/12",
      "size": 16
    },
    {
      "base": "192.168.0.0/16", 
      "size": 20
    }
  ]
} 